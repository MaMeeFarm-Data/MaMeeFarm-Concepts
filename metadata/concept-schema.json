{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "MaMeeFarm Concept Schema",
  "description": "Schema for MaMeeFarm Concept Metadata",
  "type": "object",
  "properties": {
    "concept_id": { "type": "string" },
    "title": { "type": "string" },
    "author": { "type": "string" },
    "date": { "type": "string", "format": "date" },
    "license": { "type": "string" },
    "linked_nft": { "type": "string" },
    "proof": { "type": "string" }
  },
  "required": ["concept_id", "title", "author", "date", "license"]
}
